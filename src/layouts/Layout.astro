---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="we make impossible,possible" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="logo.svg" />
		<meta name="google-site-verification" content="UNYUQNjSQ3VOpW40uzfeStDI5rCRGTfTVE3zCZkJb2o" />
		<!-- <meta name="generator" content={Astro.generator} /> -->
		<title>{title}</title>
	</head>
	<body>
		
		<Header title='PI'/>
		<main class="mt-20"> 
			<slot />
		</main>
			<Footer/>
	</body>
</html>
